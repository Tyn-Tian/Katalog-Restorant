{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-underscore-dangle": "off",
        "no-console": 0,
        "no-restricted-syntax": "off",
        "no-plusplus": "off",
        "no-unused-vars": "off",
        "no-param-reassign": "off",
        "no-restricted-globals": "off"
    }
}
